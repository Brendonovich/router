{
  "compilerOptions": {
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "moduleResolution": "Bundler",
    "module": "preserve",
    "rootDir": "src",
    "outDir": "dist/source",
    "noEmit": false,
    "declaration": true,
    "sourceMap": true,
    "target": "ES2015"
  },
  "include": ["src"]
}
